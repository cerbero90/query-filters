{
    "name": "cerbero/query-filters",
    "type": "library",
    "description": "Filter Laravel Eloquent models based on query parameters.",
    "keywords": [
        "cerbero",
        "query-filters",
        "laravel",
        "query",
        "filters",
        "eloquent"
    ],
    "homepage": "https://github.com/cerbero90/query-filters",
    "license": "MIT",
    "authors": [
        {
            "name": "Andrea Marco Sartori",
            "email": "andrea.marco.sartori@gmail.com",
            "homepage": "https://github.com/cerbero90",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^5.6|^7.0",
        "illuminate/http": "^5.4|^6.0|^7.0|^8.0",
        "illuminate/database": "^5.4|^6.0|^7.0|^8.0",
        "illuminate/console": "^5.4|^6.0|^7.0|^8.0",
        "illuminate/support": "^5.4|^6.0|^7.0|^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^3.4|^4.0",
        "phpunit/phpunit": "^5.4|^6.0|^7.0|^8.0",
        "squizlabs/php_codesniffer": "^3.0",
        "mockery/mockery": "^1.2"
    },
    "autoload": {
        "psr-4": {
            "Cerbero\\QueryFilters\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cerbero\\QueryFilters\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "Cerbero\\QueryFilters\\Providers\\QueryFiltersServiceProvider"
            ]
        }
    }
}
